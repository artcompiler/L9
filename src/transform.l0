    var ln = 1
    var col = 1

    var level = 1
    const oneIndent = 4

    function indent(delta=0) {
        level += oneIndent * delta
        return level
    }

    function func(node) {
        print("func")
        let startCol = col
        let startLn = ln
        let elts = [ ]
        elts.push({
            "tag": "tspan",
            "class": "punc",
            "id": node.id,
            "startCol": col,
            "startLn": ln,
            "stopCol": col,
            "stopLn": ln,
            "elts": ["("]
        })
        col += "(".length
        if (node.params.fixtures.length==0) {
            // do nothing
            indent(+1)   // to make the -1 below work out
        }
        else
        if (node.params.fixtures.length==1) {
            elts.push(visit(node.params.fixtures[i].name))
            indent(+1)   // to make the -1 below work out
        }
        else {
            ln += 1
            col = indent(+1)
            for (let i = 0; i < node.params.fixtures.length; i++) {
                elts.push(visit(node.params.fixtures[i].name))
                if (i < node.params.fixtures.length-1) {
                    elts.push({
                        "tag": "tspan",
                        "class": "punc",
                        "id": node.id,
                        "startCol": col,
                        "startLn": ln,
                        "stopCol": col,
                        "stopLn": ln,
                        "elts": [","]
                    })
                    col = indent()
                    ln += 1
                }
            }
        }
        elts.push({
            "tag": "tspan",
            "class": "punc",
            "id": node.id,
            "startCol": col,
            "startLn": ln,
            "stopCol": col,
            "stopLn": ln,
            "elts": [") {"]
        })
        if (node.vars.length || node.body.length) {
            ln += 1
            col = indent()
            elts.push(visit(node.vars))
            //elts.push(visit(node.attr))
            elts.push(visit(node.body))
        }
        ln += 1
        col = indent(-1)
        elts.push({
            "tag": "tspan",
            "class": "punc",
            "id": node.id,
            "startCol": col,
            "startLn": ln,
            "stopCol": col,
            "stopLn": ln,
            "elts": ["}"]
        })
        col += 1
        return {
            "tag": "tspan",
            "class": "Func",
            "id": node.id,
            "startCol": startCol,
            "startLn": startLn,
            "stopCol": col,
            "stopLn": ln,
            "elts": elts
        }
    }

