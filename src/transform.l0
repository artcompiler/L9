        })
        col += ("( ").length
        let tempCol = col
        elts.push(visit(node.head))
        col = tempCol
        elts.push({
            "tag": "tspan",
            "class": "keyword",
            "id": node.id,
            "startCol": col,
            "startLn": ln,
            "stopCol": col,
            "stopLn": ln,
            "elts": ["of"]
        })
        col += ("of ").length
        elts.push(visit(node.iterator))
        col += 1 // space after iterator
        elts.push({
            "tag": "tspan",
            "class": "punc",
            "id": node.id,
            "startCol": col,
            "startLn": ln,
            "stopCol": col,
            "stopLn": ln,
            "elts": [")"]
        })
        ln += 1
        col = tempCol - ("for ( ").length
        if (node.subclause) {
            elts.push(visit(node.subclause))
        }
        return {
            "tag": "tspan",
            "class": "comprehendFor",
            "id": node.id,
            "startCol": startCol,
            "startLn": startLn,
            "stopCol": col,
            "stopLn": ln,
            "elts": elts
        } 
    }

    function comprehendIf(node) {
